{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "@components/*": [
          "src/components/*"
        ],
        "@assets/*": [
          "src/assets/*"
        ],
        "@layouts/*": [
          "src/layouts/*"
        ],
        "@consts/*": [
          "src/consts/*"
        ]
      },
      "jsx": "react-jsx",
      "jsxImportSource": "react"
    }
  }